        <extension type="Interception"/>
        <register type="System.Data.Entity.DbContext, EntityFramework" mapTo="Analyze.Shared.DataAccess.efc_db_01_DBContext,Analyze.Shared.DataAccess"/>
        <register type="Analyze.Shared.Bussiness.Interface.ISysUserservice,Analyze.Shared.Bussiness.Interface" mapTo="Analyze.Shared.Bussiness.Services.SysUserservice,Analyze.Shared.Bussiness.Services">
            
        </register>
